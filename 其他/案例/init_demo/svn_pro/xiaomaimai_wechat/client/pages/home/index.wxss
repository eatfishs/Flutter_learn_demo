/**index.wxss**/
.container .store {
  width: 100vw;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.userinfo image {
  width: 100%;
}
.userinfo .btn-userinfo {
  width: 90%;
  background: linear-gradient(150deg, #FFAF4C 0%, #FFAF4C 100%);
  color: #fff;
  margin-top: 80rpx;
}
.userinfo .icon-wechat {
  width: 48rpx;
  height: 48rpx;
  height: linear-gradient(150deg, #FFAF4C 0%, #FFAF4C 100%);
  background-image: url("http://resource.xlwonder.wang/wxapp/xmmq/icon_signin_wechat.png");
  background-size: 100% 100%;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}
.userinfo text {
  font-size: 32rpx;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
}
.tab-content {
  margin-bottom: 75px;
}
.header-top {
  padding: 15px 0 0 0;
  height: 65px;
  width: 100%;
  background: url('data:image/png;base64,');
}
.header-top .header-img {
  display: inline-flex;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  margin-left: 15px;
  float: left;
}
.header-top .nick-name {
  display: inline-flex;
  font-size: 32rpx;
  padding: 0 10px;
  font-weight: 600;
  color: #333;
}
.header-top .fans {
  display: block;
  font-size: 24rpx;
  padding: 5px 10px;
  color: #999999;
}
.header-top .fans .follow{
  padding: 5px 10px;
}
.header-top .fans .favorite {
  margin-left: 10px;
}
.header-top .fans .share {
  padding: 0 14px;
  float:right;
  top: -15px;
  right: 2vw;
  position: relative;
  display: inline-flex;
  color: #ffffff;
  background: #FFAF4C;
  border-radius: 3px;
  border: 0;
  height: 32px;
  line-height: 32px;
}
.header-top .fans .share .icon-qr {
  padding-right: 5px;
  vertical-align: middle;
  font-weight: lighter;
}
.header-top .fans .share text {
  vertical-align: middle;
}
.photo {
  display: flex;
  position: fixed;
  bottom: 0px;
  left: 50%;
  margin-left: -35px;
  border-radius: 50%;
  box-shadow: 0 -3px 10px #cccccc;
  width: 70px;
  height: 70px;
  justify-content: center;
  align-items: center;
  z-index: 2;
  background: #ffffff;
}
.photo .camera {
  margin-top: -10px;
  font-size: 65rpx;
  color: #999999;
  font-weight: lighter;
}
.in {
  display: block;
}
.empty-placeholder {
  margin-top: 100rpx;
  background-color: white;
}