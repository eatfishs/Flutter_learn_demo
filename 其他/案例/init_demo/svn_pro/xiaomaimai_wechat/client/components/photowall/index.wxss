.photo-wall {
  padding: 5px 0;
  width: 100%;
  overflow: hidden
}
.photo-wall .date {
  color: #666666;
  margin: 5px 15px;
  width: 100vw;
  font-size: 26rpx;
  font-weight: 500;
}
.photo-wall .count{
  position: relative;
  display: inline-flex;
  color:#FFA739;
}
.photo-wall image {
  max-width: 100%;
  max-height: 120px;
  text-align: right;
}
.photo-wall .multiimage {
  position: relative;
  width: 50rpx;
  height: 50rpx;
  top: -120px;
  left: -60px;
  padding:3 3 3 3px;
  display: inline-flex;
  background: rgba(0,0,0,0.6);
  justify-content: center;
  align-items: center;
}
.photo-wall .multiimage image{
  max-width: 25rpx;
  max-height: 25rpx;
}

.photo-wall .multiprice {
  position: relative;
  top: -17px;
  padding: 0 5px;
  color: #ffffff;
  float: left;
  width: 80px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: inline-flex;
  background: rgba(0, 0, 0, 0.6);
  font-weight: bolder;
  font-size: 20rpx;
}
.photo-wall .multiprice .unit {
  font-weight: bolder;
  font-size: 20rpx;
}
.photo-wall .price {
  position: relative;
  top: -17px;
  padding: 0 5px;
  color: #ffffff;
  float: left;
  max-width: 80px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: inline-flex;
  background: rgba(0, 0, 0, 0.6);
  font-weight: bolder;
  font-size: 24rpx;
}
.photo-wall .price .unit {
  font-weight: bolder;
  font-size: 24rpx;
}
.photo-wall .favorite {
  position: relative;
  display: inline-block;
  top: -25px;
}
.photo-wall .favorite .icon-star-o {
  position: relative;
  right: 0px;
  font-size: 34rpx !important;
  color: #666666;
}
.photo-wall .favorite .icon-star {
  font-size: 34rpx !important;
  position: relative;
  right: 0px;
  color: #FFA739;
}
.photo-wall .add-to-cart,
.photo-wall merge {
  position: absolute;
  margin-top: -115px;
  margin-left: -10px;
  display: inline-flex;
  width: 15px;
  height: 15px;
  border-radius: 50%;
  background: transparent;
  justify-content: center;
  align-items: center;
}
.photo-wall .add-to-cart2{
  position: relative;
  top: -115px;
  right: -38px;
  display: inline-flex;
  width: 15px;
  height: 15px;
  border-radius: 50%;
  background: #ffffff;
  justify-content: center;
  align-items: center;
}
.photo-wall .add-to-cart .icon-plus-circle,
.photo-wall .add-to-cart2 .icon-plus-circle,
.photo-wall merge .icon-plus-circle {
  display: inline-flex;
  width: 25px;
  height: 25px;
  color: #999999;
  font-size: 40rpx;
}
.photo-wall .add-to-cart .icon-correct,
.photo-wall .add-to-cart2 .icon-correct,
.photo-wall merge .icon-correct {
  display: inline-flex;
  width: 25px;
  height: 25px;
  color: #FFAF4C;
  font-size: 40rpx;
}

.photo-wall .btn-select {
  font-size: 26rpx;
  color: #2377C3;
  display: inline-flex;
  position: fixed;
  top:40rpx;
  right: 10px;
  z-index:100;
}
.photo-wall .cart-list {
  display: none;
  position: fixed;
  width: 100%;
  min-height: 200px;
  max-height: 365px;
  overflow-y: auto;
  bottom: 60px;
  background: #ffffff;
  z-index: 5;
  box-shadow: 0 0 20px rgba(153, 153, 153, 0.5);
}
.photo-wall .cart-list .cart-title {
  color: #999999 !important;
  margin: 5px 15px;
  padding: 10px 0;
  border-bottom: 1px solid #e3e3e3;
  font-size: 26rpx;
}
.photo-wall .cart-list .cart-title .clear {
  display: inline-flex;
  float: right;
}
.photo-wall .cart-list .cart-title .clear .icon-rabbish {
  margin-right: 5px;
  vertical-align: bottom;
}
.photo-wall .cart-list .cart-content {
  display: flex;
  flex-direction: column;
  width: 100%;
}
.photo-wall .cart-list .cart-content .card {
  display: flex;
  flex-direction: row;
  width: 100%;
  padding: 5px 0;
}
.photo-wall .cart-list .cart-content .no {
  display: inline-flex;
  width: 10%;
  justify-content: center;
  align-items: center;
  color: #999999;
}
.photo-wall .cart-list .cart-content .product-image {
  display: inline-flex;
  width: 30%;
  justify-content: center;
  align-items: center;
}
.photo-wall .cart-list .cart-content .product-image image {
  width: 95px;
  height: 95px;
}
.photo-wall .cart-list .cart-content .sub-total {
  justify-content: center;
  align-items: center;
  display: inline-flex;
  width: 30%;
  color: #666666;
}
.photo-wall .cart-list .cart-content .stepper {
  justify-content: center;
  align-items: center;
  display: inline-flex;
  width: 30%;
  color: #666666;
}
.photo-wall .cart-list .cart-content .stepper .btn-plus {
  width: 30px;
}
.photo-wall .cart-list .cart-content .stepper .btn-minus {
  width: 30px;
}
.photo-wall .cart-list.in {
  display: block;
}
.photo-wall .merge {
  position: fixed;
  display: flex;
  flex-wrap: wrap; 
  justify-content: space-between;
  width: 100%;
  height: 60px;
  bottom: 0;
  background: #f8f8f8;
  box-shadow: 0 0 20px rgba(153, 153, 153, 0.5);
  z-index: 10;
  color: #666666;
}
.photo-wall .merge .btn-merge {
  display: inline-flex;
  width: 30%;
  justify-content: center;
  align-items: center;
  height: 60px;
  font-size: 32rpx;
}
.photo-wall .merge .btn-merge .icon-merge {
  font-size: 34rpx;
  margin-right: 10px;
  font-weight: 700;
}
.photo-wall .shopping-cart {
  position: fixed;
  width: 100%;
  height: 60px;
  bottom: 0;
  background: #ffffff;
  box-shadow: 0 0 20px rgba(153, 153, 153, 0.5);
  z-index: 10;
}
.photo-wall .shopping-cart .icon-cart {
  position: fixed;
  bottom: 0;
  color: #ff9100;
  font-size: 2.7rem;
  padding: 10px;
  width:120rpx;
  height: 120rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  border-radius: 40px;
  background: #ffffff;
  box-shadow: 0 -10px 20px #ededed;
}
.photo-wall .shopping-cart .icon-cart image{
  width:120rpx;
  height: 120rpx;
}
.photo-wall .shopping-cart .total {
  margin-left: 80px;
  height: 60px;
}
.photo-wall .shopping-cart .total .account {
  color: #EE5050;
  font-size: 36rpx;
  padding: 5px 0;
}
.photo-wall .shopping-cart .total .message {
  color: #999999;
  font-size: 24rpx;
  padding-bottom: 5px;
}
.photo-wall .shopping-cart .checkout {
  display: inline-flex;
  line-height: 60px;
  float: right;
  width: 150px;
  justify-content: center;
  letter-spacing: 5px;
  border-radius: 0;
  background: linear-gradient(to bottom right, #FFC63F, #FFA739);
  color: #ffffff;
  font-size: 34rpx;
}
.backdrop {
  position: absolute;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 3;
  width: 100%;
  height: 100%;
  display: none;
  top: 0;
}
.backdrop.in {
  display: block;
}
.empty-cart {
  width: 100%;
  height: 100%;
  text-align: center;
  padding-top: 40px;
}
.empty-cart image {
  position: relative;
  left: 50%;
  display: flex;
  flex: 1;
  width: 100px;
  height: 100px;
  margin-left: -50px;
}
.empty-cart .message {
  width: 100%;
  font-size: 26rpx;
  color: #999999;
}
.hide {
  display: none;
}
.upload {
  background: #ffffff;
  position: fixed;
  bottom: 100px;
  right: 0;
  border-radius: 50%;
  border: 1px solid #FFAD4C;
  box-shadow: 0 0 10px #ededed;
}
.upload .icon-camera {
  padding: 5px;
  font-size: 40rpx;
  color: #FFAD4C;
}
.upload-backdrop {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  z-index: 2000;
}
.upload-backdrop .uploading {
  position: fixed;
  width: 80%;
  height: 100px;
  margin: 0 5%;
  top: 50%;
  margin-top: -50px;
  background: #ffffff;
  border-radius: 5px;
  padding: 0 15px;
}
.upload-backdrop .uploading .progress-bar {
  margin-top: 40px;
  width: 100%;
}
.upload-backdrop .uploading .uploading-text {
  width: 100%;
  text-align: center;
  margin-top: 10px;
  font-size: 26rpx;
}
.photo {
  display: flex;
  position: fixed;
  bottom: 0px;
  left: 50%;
  margin-left: -35px;
  border-radius: 50%;
  box-shadow: 0 -3px 10px #cccccc;
  width: 70px;
  height: 70px;
  justify-content: center;
  align-items: center;
  z-index: 2;
  background: #ffffff;
}
.photo .camera {
  margin-top: -10px;
  font-size: 65rpx;
  color: #999999;
  font-weight: lighter;
}

.sizeModal-img {
  width: 30rpx;
  height: 30rpx;
}