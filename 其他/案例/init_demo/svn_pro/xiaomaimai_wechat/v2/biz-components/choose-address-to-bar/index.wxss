@import "../../base.wxss";

.address-bar {
  position: relative;
  box-sizing: border-box;
  overflow: hidden;
  background-color: #fff;
}

.address-bar::before,
.address-bar::after {
  position: absolute;
  content: "";
  width: 100%;
  height: 4px;
  background: transparent
    url("data:image/png;base64,");
  background-size: cover;
  left: 0;
  right: 0;
  z-index: 1;
}

.address-bar::before {
  top: 0;
}

.address-bar::after {
  bottom: 0;
}

.null-cell {
  padding-top: 30px !important;
  padding-bottom: 30px !important;
}

.null-title {
  padding-left: 5px;
}
