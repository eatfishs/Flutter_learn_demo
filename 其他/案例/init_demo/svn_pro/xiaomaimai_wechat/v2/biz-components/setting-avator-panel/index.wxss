


.flex-row-center {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.flex-row-top-left {
  display: flex;
  flex-direction: row;
  align-items: left;
  justify-content: flex-start;
}

.flex-row-bottom-left {
  display: flex;
  flex-direction: row;
  align-items: left;
  justify-content: flex-start;
}

.flex-row-center-left {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}

.flex-row-center-right {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.flex-column-center {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.flex-column-right {
  display: flex;
  flex-direction: row;
  align-items: right;
  justify-content: flex-end;
}

.banner-bg {
  width: 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
}

.userinfo {
  position: relative;
  width: 100vw;
  height: 180px;
  /* background: linear-gradient(to bottom, #ffffff, #ededed);
    background-size: auto; */
  background-image: url(data:image/png;base64,);
  background-position: no-repeat;
  background-size: cover;
  color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
}

.userinfo .bottom-circle-bar {
  height: 15px;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  background-color: white;
  border-top-left-radius: 15px;
  border-top-right-radius: 15px;
}

.userinfo .avator-container {
  position: relative;
}

.userinfo .icon-editor {
  width: 18px;
  height: 18px;
}

.userinfo .avator-container .icon-editor {
  position: absolute;
  right: 0rpx;
  bottom: 0;
}

.userinfo .export .icon-editor {
  margin-right: 8px;
}

.avatar {
  box-sizing: border-box;
  display: block;
  width: 75px;
  height: 75px;
  border-radius: 50%;
  border: 2px solid#fff;
}

.userinfo .info {
  flex-direction: column;
}

.userinfo .info .nick-name {
 font-size: 18px;
color: #666;
padding: 10px 0;
}

.userinfo .info .icon-phone {
  width: 18rpx;
  height: 26rpx;
  background-image: url("http://resource.xlwonder.wang/wxapp/xmmq/icon_my_phone.png");
  background-size: 100% 100%;
  display: inline-block;
  margin-right: 10rpx;
  font-weight: normal;
}

.userinfo .info .phoneNumber {
  font-size: 28rpx;
  line-height: 30rpx;
  color: white;
  display: inline-block;
}

.userinfo .info .icon-level {
  width: 30rpx;
  height: 30rpx;
  margin-left: 6rpx;
}

.userinfo .icon-qr-code {
  width: 40rpx;
  height: 40rpx;
  border-radius: 5rpx;
  background-image: url("http://resource.xlwonder.wang/wxapp/xmmq/icon_my_code.png");
  background-size: 100% 100%;
}

.userinfo .icon-top-left {
  position: absolute;
  left: 15px;
  top: 26px;
  right: 15px;
}

.balance {
  background: #ffac45;
  box-shadow: 0 0 10px rgba(153, 153, 153, 0.4);
  padding: 10px 15px;
  color: #333;
  border-top-left-radius: 30px;
  border-bottom-left-radius: 30px;
  top: 5px;
  right: 0;
  position: absolute;
}

.export {
  background: #fff;
  box-shadow: 0 0 10px rgba(153, 153, 153, 0.4);
  padding: 10px 15px;
  color: #3a3a3a;
  font-size: 14px;
  border-top-left-radius: 30px;
  border-bottom-left-radius: 30px;
  top: 42px;
  right: 0;
  position: absolute;
  z-index: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.export .icon-download {
  color: #ffac45;
  font-size: 16px;
  margin-right: 2px;
}
