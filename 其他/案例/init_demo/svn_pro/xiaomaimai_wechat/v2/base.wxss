.btn-main {
  position: relative;
  z-index: 100;
  line-height: 50px;
  background: var(--button-main-background-image);
  text-align: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  color: #fff;
  height: 50px;
  width: 100%;
  letter-spacing: 4px;
  transition: opacity 0.2s;
  transition: opacity var(--animation-duration-fast, 0.2s);
  border-radius: 2px;
  border-radius: var(--button-border-radius, 2px);
}
.btn-main[disabled]{
  color: #fff !important;
}
.btn-main[disabled]:before{
  opacity: 0.4;
  background-color: #fff;
}
.btn-bg-transparent {
  background: transparent;
}

.btn-main:before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  border: inherit;
  border-radius: inherit;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  opacity: 0;
  content: " ";
  z-index: 10;
  background-color: #000;
}
.btn-main-image {
  width: 100%;
  height: 100%;
}

.btn-mian-conver-view {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}
.btn-mian-conver-view:active {
  background-color: #000;
  opacity: 0.15;
}
.btn-main:after {
  border-width: 0;
  z-index: 1000;
}

.btn-active:before {
  opacity: 0.15;
}

.form-control {
  display: block;
  box-sizing: border-box;
  width: 100%;
  height: calc(1.5em + 0.75rem + 2px);
  padding: 0.375rem 0.75rem;
  font-size: 1rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: #f3f3f3;
  background-clip: padding-box;
}
.border-box{
  box-sizing: border-box;
