/**index.wxss**/

.store {
  width: 100vw;
}
.searchbox-ab {
}
.searchbox-bar {
  position: fixed !important;
  width: 100vw;
  z-index: 1;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.userinfo image {
  width: 100%;
}

.userinfo .btn-userinfo {
  width: 90%;
  background: linear-gradient(150deg, #ffaf4c 0%, #ffaf4c 100%);
  color: #fff;
  margin-top: 80rpx;
}

.userinfo .icon-wechat {
  width: 25px;
  height: 25px;
  height: linear-gradient(150deg, #ffaf4c 0%, #ffaf4c 100%);
  background-image: url("http://resource.xlwonder.wang/wxapp/xmmq/icon_signin_wechat.png");
  background-size: 100% 100%;
  display: inline-block;
  vertical-align: middle;
  margin-right: 5px;
  position: absolute;
}

.userinfo text {
  font-size: 16px;
}

.userinfo-avatar {
  width: 65px;
  height: 65px;
  margin: 10px;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.header-top {
  padding: 16px 0 0 0;
  height: 80px;
  width: 100%;
  background: url("data:image/png;base64,");
}

.moments-view {
  width: 100%;
  background: url("data:image/png;base64,");
  background-repeat: repeat-x;
  background-position: top;
}
.moments-refresher-view {
  position: relative;
}
.moments-refresher {
  position: relative;
}

.scroll-wall-view {
  width: 100%;
  background-color: #f7f7f7;
}

.header-top .header-img {
  display: inline-flex;
  border-radius: 50%;
  width: 50px;
  height: 50px;
  overflow: hidden;
  margin-left: 16px;
  float: left;
}

.header-top .nick-name {
  display: inline-flex;
  font-size: 16px;
  padding: 0 10px;
  font-weight: 600;
  color: #333;
}

.header-top .fans {
  display: block;
  font-size: 12px;
  padding: 5px 10px;
  color: #999;
}

.header-top .fans .follow {
  padding: 5px 10px;
}

.header-top .fans .favorite {
  margin-left: 10px;
}

.header-top .fans .share {
  padding: 0 15rpx;
  float: right;
  top: -150px;
  right: 2vw;
  position: relative;
  display: inline-flex;
  color: #fff;
  background: #ffaf4c;
  border-radius: 4px;
  border: 0;
  height: 32px;
  line-height: 32px;
}

.header-top .fans .share .icon-qr {
  padding-right: 5px;
  vertical-align: middle;
  font-weight: lighter;
}

.header-top .fans .share text {
  vertical-align: middle;
}

.in {
  display: block;
}

.failicon {
  margin-top: 30px;
}

.notitle {
  visibility: hidden;
}

.edituser-dialog {
  top: 0 !important;
  font-size: 16px;
  line-height: 24px;
  background-color: #fff;
  width: 320px;
  width: var(--dialog-width, 320px);
}

.edituser-dialog-header {
  text-align: center;
  position: absolute;
  top: -30px;
  left: 50%;
  margin-left: -30px;
}

.edituser-dialog-body {
  padding: 50px 0px 20px;
  padding: var(--edituser-dialog-body-padding, 50px 0px 20px);
}

.edituser-dialog-close {
  position: absolute;
  top: 15rpx;
  right: 15px;
}

.van-field {
  display: flex;
}

.qrcodeimg {
  width: 100%;
  margin-bottom: 10px;
}

.qrcodeimg .image {
  background-color: #f7f7f7;
}

.avatar {
  box-sizing: border-box;
  display: block;
  width: 75px;
  height: 75px;
  border-radius: 50%;
  border: 2px solid#fff;
}

.flex {
  display: flex;
  align-items: center;
  justify-content: left;
}

.flex-field-image {
  position: relative;
  display: block;
  box-sizing: border-box;
  width: 100%;
  margin: 0;
  padding: 0;
  line-height: inherit;
  text-align: left;
  background-color: initial;
  border: 0;
  resize: none;
  color: #323233;
  color: var(--field-input-text-color, #323233);
}

.home-tab .van-tabs__wrap {
  position: relative;
  z-index: 200;
}
.home-tab .van-hairline--top-bottom::after {
  border-top: none !important;
}

.searchbox-index--search-input .van-search__content {
  background-color: #fff;
}

.btn-select {
  color: #2377c3;
  width: 60px;
  background: transparent;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  position: relative;
  z-index: 200;
}

.search_no {
  position: absolute;
  width: 100%;
}
.avatar-border-6 {
  border: 6px solid #fff;
}
page {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.mommentbar {
  background-color: #f7f7f7 !important;
}
.mommentbar .van-search__content {
  background: #fff !important;
}
.mommentbar-btn {
  color: #2377c3;
}

.teach-bar {
  position: relative;
  background-color: #eff6ff;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 40px;
  line-height: 40px;
  color: #1d72be;
  z-index: 1000;
}
.teach-bar:before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  border: inherit;
  border-radius: inherit;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  opacity: 0;
  content: " ";
  background-color: #000;
  background-color: var(--black, #000);
  border-color: #000;
  border-color: var(--black, #000);
}

.teach-bar_hover:before {
  opacity: 0.15;
}
.teach-bar-inner {
  display: flex;
  justify-content: center;
  align-items: center;
}
.teach-close {
  position: absolute;
  right: 15px;
  top: 2px;
}
