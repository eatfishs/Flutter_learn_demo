page {
  background-color: #f1f1f1;
}
.prew_video {
  padding: 100% 0 0;
  position: relative;
}
.prew_video video {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
.info-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #777;
}
.pricture-wrap {
  background-color: #fff;
}
.pricture-wrap image {
  width: 100%;
}
.empty-wrap {
  background-color: #fff;
  display: flex;
  justify-content: center;
  padding: 20px 0;
  color: #bbb;
}
.empty-wrap > view {
  display: inline-flex;
  align-items: center;
}
.footer {
  height: 50px;
}
.price text {
  font-size: 16px;
}
.cell {
  padding: 6px 16px !important;
  line-height: 22px !important;
}
.share-btn {
  margin-right: -16px;
  border-radius: 15px 0 0 15px !important;
}
.shear-btn-inner {
  display: inline-flex;
  align-items: center;
}
.share-icon {
  transform: translate3d(0, 0, 0);
}
.products-wrap {
  padding: 0 16px;
  display: flex;
  align-content: stretch !important;
  flex-wrap: wrap !important;
  margin: 0 -4px;
  text-align: left;
}
.products-wrap > view {
  width: 33.33%;
  box-sizing: border-box;
  padding: 0 4px;
}
.swiper {
  background-color: #fff;
  height: 200px;
}

.notice {
  height: 30px;
  line-height: 30px;
  text-align: center;
  margin-top: -30px;
}
.notice-def {
  background: rgba(68, 68, 68, 0.9);
  color: #fff;
}
.notice-warning {
  background: rgba(255, 222, 222, 0.9);
  color: #f00;
}
.comp {
  bottom: 80px !important;
}
.page-footer-fixed {
  background-color: #fff;
}
.btn {
  height: 40px !important;
  font-size: 16px !important;
}
.btn.van-button--disabled {
  filter: grayscale(100%);
}
.btn.van-button--disabled::before {
  opacity: 0 !important;
}
.footer-bar {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 100%;
  padding: 0 16px;
}
.group-btn {
  flex: 1;
}
.cart-tab {
  color: #666;
  line-height: 16px;
  font-size: 12px;
  margin-right: 16px;
}
.group-btn  .btn-cart {
  border: none;
  border-radius: 20px 0 0 20px;
 	background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
  color: #fff;
}
.group-btn  .btn-buy{
  border: none;
  border-radius: 0 20px 20px 0;
 background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
  color: #fff;
}
.favorite-icon{

  width: 65px;
  display: inline-flex;
  align-items: center;
  justify-content: flex-end;
}
