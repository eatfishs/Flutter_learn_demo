.footer {
    position: fixed;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: 10;
    box-sizing: border-box;
    background: #fff;

}

.footer .tab-bar {
    border-top: 1px solid#c9c9c9;
    padding-bottom: 0 !important;
}

.selected {
    color: #FFAF4C;
    text-align: center;
    font-size: 12px;
}

.normal {
    color: gray;
    text-align: center;
    font-size: 12px;
}

.icon-img {
    margin: -7px 0;
    height: 34px;
    width: 34px;
}

.camera {
    margin:-24px 0 -6px 0;
    width: 50px;
    height: 50px;
}
.shopping-cart {
  width: 100%;
  height: 60px;
  background: #ffffff;
  box-shadow: 0 0 20px rgba(153, 153, 153, 0.5);
  z-index: 10;
}

.shopping-cart .icon-cart {
  /*-webkit-filter: drop-shadow(0 0 40rpx rgba(153,153,153,0.5));*/
  position: absolute;
  width: 80px;
  height: 90px;
  top: -30px;

}

.shopping-cart .total {
  margin-left: 80px;
  height: 60px;
}

.shopping-cart .total .account {
  color: #EE5050;
  font-size: 18px;
  padding: 5px 0;
}

.shopping-cart .total .message {
  color: #999999;
  font-size: 12px;
  padding-bottom: 5px;
}

.shopping-cart .checkout {
  display: inline-flex;
  line-height: 60px;
  float: right;
  width: 150px;
  justify-content: center;
  letter-spacing: 5px;
  border-radius: 0;
  background: linear-gradient(to bottom right, #FFC63F, #FFA739);
  color: #ffffff;
  font-size: 16px;
}
