@import "miniprogram_npm/@vant/weapp/common/index.wxss";
@import "./base.wxss";

page {
  --blue: #f00;
  --toast-background-color: pink;
  --button-primary-background-color: #ffaf4c;
  --button-primary-border-color: #ffaf4c;
  --checkbox-checked-icon-color: #ffaf4c;
  --switch-on-background-color: #ffaf4c;
  --radio-checked-icon-color: #ffaf4c;
  --radio-label-color: #999;
  --tag-primary-color: #fff0dd;
  --tag-text-color: #ffaf4c;

  --button-default-border-color: #fff;
  --button-secondary-background-color: #f66;
  --button-secondary-border-color: #f66;
  --button-secondary-color: #fff;
  --button-large-height: 50px;
  --button-main-background-image: url(data:image/png;base64,);
  --dialog-border-radius: 2px;
  --play-icon: url(data:image/png;base64,);
  font-size: 14px;
  box-sizing: border-box;
}

.hide {
  display: none;
}

.page-body {
  padding: 0px 16px;
  flex: 1 1 auto;
  box-sizing: border-box;
  width: 100%;
}

.page-footer-fixed {
  position: fixed;
  bottom: 0;
  width: 100vw;
  height: 50px;
  text-align: center;
  line-height: 50px;
  color: #fff;
  z-index: 10;
}

scroll-view {
  box-sizing: border-box;
}

.prew_video {
  padding: 75% 0 0;
  position: relative;
  margin-bottom: 6px;
}

.prew_image {
  padding: 100% 0 0;
  position: relative;
  margin-bottom: 6px;
}

.prew_video video,
.prew_image video,
.prew_image image {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}

.prew_image .rabbish-warp {
  position: absolute;
  z-index: 1;
  right: 0;
  top: 0;
  width: 25px;
  background: rgba(0, 0, 0, 0.5);
  color: #fff;
  height: 25px;
  text-align: center;
  padding-top: 2px;
}

.prew_image .icon-plus {
  position: absolute;
  font-size: 50px;
  color: #d1d1d1;
  background: #f8f8f8;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  top: 0;
  font-weight: lighter;
}

.prew_image .prew_price {
  position: absolute;
  bottom: 0rpx;
  left: 0;
  right: 0;
  padding: 0 8px;
  line-height: 20px;

  color: #fff;
  background: rgba(0, 0, 0, 0.6);
  font-size: 16px;
  font-weight: normal;
}

.van-button--large {
  font-size: 18px !important;
}
.van-button--dotted {
  background: #ffffff;
  color: #999;
  border: 1px dotted #979797;
}
.van-button--secondary {
  color: #fff;
  color: var(--button-secondary-color, #fff);
  background-color: #f66;
  background-color: var(--button-secondary-background-color, #f66);
  border: 1px solid #f66;
  border: 1px solid var(--button-secondary-border-color, #fff);
}

.one-content {
  width: 100%;
}
